<?xml version='1.0' encoding='utf-8'?>
<widget id="org.aitour.scene" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AiTourGuide</name>
    <description>
        Ai tourist guide
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        AiTour team
    </author>
    <icon density="xxxhdpi" src="res/android/xxxhdpi.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <plugin name="cordova-plugin-baidu-geolocation" spec="https://github.com/ETENG-OSP/cordova-plugin-baidu-geolocation.git">
            <variable name="API_KEY" value="h7h5KR0fYpjV3S6k6nWn5D9tjfZbr4Ct" />
        </plugin>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1" />
    <plugin name="cordova-plugin-fullscreen" spec="^1.2.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-baidu-geolocation" spec="git+https://github.com/ETENG-OSP/cordova-plugin-baidu-geolocation.git">
        <variable name="API_KEY" value="h7h5KR0fYpjV3S6k6nWn5D9tjfZbr4Ct" />
    </plugin>
</widget>
